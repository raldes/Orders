{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",

  "AzureServiceBusEnabled": false,
  "SubscriptionClientName": "Inventory",
  "GracePeriodTime": "1",
  "CheckUpdateTime": "30000",
  "ApplicationInsights": {
    "InstrumentationKey": ""
  },
  "EventBusRetryCount": 5,
  "EventBusConnection": "rabbitmq",
  "EventBusPort": 5672,
  "EventBusUserName": "guest",
  "EventBusUserPassword": "guest",
  "UseVault": false,
  "Vault": {
    "Name": "inventory",
    "ClientId": "your-client-id",
    "ClientSecret": "your-client-secret"
  }
}
